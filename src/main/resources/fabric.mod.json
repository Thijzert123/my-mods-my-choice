{
	"schemaVersion": 1,
	"id": "mymodsmychoice",
	"version": "${version}",
	"name": "My Mods, My Choice",
	"description": "Enable and disable mods via an in-game configuration screen",
	"authors": [
		"Thijzert"
	],
	"contact": {
		"homepage": "https://github.com/Thijzert123/my-mods-my-choice",
		"sources": "https://github.com/Thijzert123/my-mods-my-choice"
	},
	"license": "MIT",
	"icon": "assets/mymodsmychoice/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.thijzert123.mymodsmychoice.MyModsMyChoice"
		],
		"client": [
			"io.github.thijzert123.mymodsmychoice.MyModsMyChoiceClient"
		],
		"modmenu": [
        		"io.github.thijzert123.mymodsmychoice.config.InGameConfig"
		]
	},
	"languageAdapters": {
		"dummy_language_adapter": "settingdust.preloadingtricks.DummyLanguageAdapter"
	},
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*",
		"yet_another_config_lib_v3": ">=3.6.6+1.21.5-fabric"
	},
	"jars": [
		{"file": "jackson-core-2.19.0.jar"},
		{"file": "jackson-databind-2.19.0.jar"},
		{"file": "jackson-annotations-2.19.0.jar"},
		{"file": "preloading-tricks-1.2.3.jar"}
	]
}